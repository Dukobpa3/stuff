<?xml version="1.0" encoding="utf-8" ?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<languages>
		<language name="text">
			<lexer name="cpp" style-bits="7" />
			<character-class><![CDATA[qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM0123456789]]></character-class>
			<file-extensions>*</file-extensions>
			<!-- COLORING_START -->
			<editor-style caret-fore="0xD5C175" caretline-back="0x323C3F" selection-fore="0x000000" selection-back="0xC0C0C0" margin-fore="0x808080" margin-back="0x808080" marker-fore="0x808080" marker-back="0x293134" />
			<use-styles>
				<style name="default" fore="0xE0E2E4" back="0x293134" size="9" font="Courier New" />
				<style name="comment" />
				<style name="commentline" />
				<style name="commentdoc" />
				<style name="number" />
				<style name="word" />
				<style name="string" />
				<style name="character" />
				<style name="uuid" />
				<style name="preprocessor" />
				<style name="operator" />
				<style name="identifier" />
				<style name="stringeol" />
				<style name="verbatim" />
				<style name="regex" />
				<style name="commentlinedoc" />
				<style name="word2" />
				<style name="commentdockeyword" />
				<style name="commentdockeyworderror" />
				<style name="globalclass" />
				<style name="gdefault" fore="0x81969A" />
				<style name="linenumber" fore="0x81969A" />
				<style name="bracelight" fore="0x72898D" back="0xe2F393C" bold="true" />
				<style name="bracebad" bold="true" />
				<style name="controlchar" fore="0xffffff" />
				<style name="indentguide" fore="0xc0c0c0" />
				<style name="lastpredefined" fore="0x666666" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>
